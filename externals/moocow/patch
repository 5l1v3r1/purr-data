+  case 3:
+#line 81 "gfsmRegex.tab.y"
+    { (yyval.fsm)=gfsm_regex_compiler_label_fsm(my_compiler, (yyvsp[(1) - (1)].u)); }
+    break;
+
+  case 4:
+#line 84 "gfsmRegex.tab.y"
+    { (yyval.fsm)=gfsm_regex_compiler_prepend_lab(my_compiler, (yyvsp[(1) - (2)].u), (yyvsp[(2) - (2)].fsm)); }
