- ##    ext1_la_SOURCES = ...
- ##    ext1_la_LDFLAGS = -module
- ##  + still unclear how to get *.$(PDEXT) targets built from *.la
- dnl AC_DISABLE_STATIC
- dnl AC_LIBTOOL_DLOPEN
- dnl AC_PROG_LIBTOOL
