- case "${am__api_version}" in
-  1.[[0-4]]*)
-    AC_MSG_RESULT([automake v${ap__api_version}: on install])
-    pd_buildext=""
-    ;;
-  *)
-    AC_MSG_RESULT([automake v${am__api_version}: on build])
-    pd_buildext="\$(EXEEXT)"
-    ;;
- esac
- AC_SUBST(pd_buildext)
