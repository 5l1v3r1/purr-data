- ##----------- `uname -s`: kernel name
- AC_MSG_CHECKING([target system])
- uname_s="`uname -s`"
-
- case "$uname_s" in
-   Linux)
-     AC_MSG_RESULT(linux)
-     ;;
-   Darwin)
-     AC_MSG_RESULT(darwin)
-     ;;
-   MINGW32_NT-*)
-     ##-- build farm: `uname -s` = MINGW32_NT-5.1
-     AC_MSG_RESULT(win32)
-     uname_s="MINGW32_NT" ##-- normalize
-     ;;
